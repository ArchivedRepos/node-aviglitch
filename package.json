  "repository": {
    "type": "git",
    "url": "git://github.com/fand/node-aviglitch.git"
  },
  "keywords": [
    "avi",
    "video",
    "glitch",
    "datamosh"
  ],
  "author": "Jun Fan <fand@kde.cs.tsukuba.ac.jp> (http://gmork.in/)",
  "license": "MIT",
  "dependencies": {
    "commander": "^2.2.0",
    "fs-extra": "^0.9.1",
    "optimist": "^0.6.1"
  },
  "devDependencies": {
    "chai": "^1.9.1",
    "coffee-script": "^1.7.1",
    "coveralls": "^2.10.0",
    "gulp": "^3.8.0",
    "gulp-coffee": "^2.0.1",
    "gulp-istanbul": "^0.2.0",
    "gulp-mocha": "^0.4.1",
    "gulp-plumber": "^0.6.3",
    "istanbul": "^0.2.11",
    "mocha": "^1.20.1",
    "mocha-lcov-reporter": "0.0.1"
  }
